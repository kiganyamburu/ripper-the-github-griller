{
  "name": "genkit",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/genkit/src",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/genkit/**/*.ts"],
        "fix": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/apps/genkit"],
      "options": {
        "jestConfig": "apps/genkit/jest.config.ts",
        "passWithNoTests": true,
        "coverageDirectory": "coverage/apps/genkit"
      }
    },
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "main": "apps/genkit/src/index.ts",
        "outputPath": "dist/apps/genkit",
        "outputFileName": "index.js",
        "project": "apps/genkit/package.json",
        "tsConfig": "apps/genkit/tsconfig.json",
        "assets": ["apps/genkit/package.json"],
        "platform": "node",
        "bundle": false,
        "thirdParty": false,
        "generatePackageJson": true,
        "skipTypeCheck": false
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true
        }
      }
    },
    "buildDev": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "main": "apps/genkit/src/index.ts",
        "outputPath": "dist/apps/genkit",
        "outputFileName": "index.js",
        "project": "apps/genkit/package.json",
        "tsConfig": "apps/genkit/tsconfig.dev.json",
        "assets": ["apps/genkit/package.json"],
        "platform": "node",
        "bundle": false,
        "thirdParty": false,
        "minify": false,
        "generatePackageJson": true,
        "skipTypeCheck": false
      }
    },
    "serve": {
      "command": "pwsh -File apps/genkit/serve.ps1"
    },
    "deploy": {
      "command": "firebase deploy --only functions"
    }
  },
  "tags": []
}
